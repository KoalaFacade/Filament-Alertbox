{
    "name": "koalafacade/filament-alertbox",
    "description": "Alert box field for Filament forms",
    "type": "library",
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.13",
        "filament/filament": "^2.16"
    },
    "require-dev": {
        "laravel/pint": "^1.3",
        "nunomaduro/larastan": "^2.3",
        "nunomaduro/collision": "^6.4",
        "orchestra/testbench": "^7.18",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.3",
        "pestphp/pest-plugin-livewire": "^1.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "KoalaFacade\\FilamentAlertBox\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KoalaFacade\\FilamentAlertBox\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Stiven Trizky Katuuk",
            "email": "pemudakoding@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "KoalaFacade\\FilamentAlertBox\\FilamentAlertBoxServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    }
}
